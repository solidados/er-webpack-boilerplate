{
  "root": true,
  "env": { "es6": true, "browser": true, "node": true },
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "airbnb-base",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parserOptions": { "ecmaVersion": "latest", "sourceType": "module" },
  "rules": { "import/extensions": "off", "import/prefer-default-export": "off" },
  "plugins": ["prettier"]
}
